ImageLabel.Size = UDim2.new(0.349999994, 0, 0.400000006, 0)
ImageLabel.Visible = true
ImageLabel.Image = "rbxassetid://13507277926"

local JumpscareSound = Instance.new("Sound", workspace)
JumpscareSound.SoundId = "rbxassetid://5567523008"
JumpscareSound.PlaybackSpeed = 0.5
JumpscareSound.TimePosition = 0.25
JumpscareSound.Volume = 1

local Distorsion = Instance.new("DistortionSoundEffect", JumpscareSound)
Distorsion.Level = 0.9

local PitchShift = Instance.new("PitchShiftSoundEffect", JumpscareSound)
PitchShift.Octave = 1.25

JumpscareSound:Play()
task.spawn(function()
	while ScreenGui do task.wait(0.04)
		Frame.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
		ImageLabel.Rotation = Random.new():NextNumber(-50, 50)
		ImageLabel.Position = UDim2.new(Random.new():NextNumber(0, 0.65), 0, Random.new():NextNumber(0, 0.6), 0) task.wait(0.04)
		Frame.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
		ImageLabel.Rotation = Random.new():NextNumber(-50, 50)
		ImageLabel.Position = UDim2.new(Random.new():NextNumber(0, 0.65), 0, Random.new():NextNumber(0, 0.6), 0)
	end
end)

task.wait(0.7)

ScreenGui:Destroy()
JumpscareSound:Destroy()
game.Players.LocalPlayer.PlayerGui.ScreenGui:Destroy()
	end
end

