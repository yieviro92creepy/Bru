        if game.Players.LocalPlayer.Character.Humanoid.MoveDirection ~= Vector3.new(0,0,0) then
        game.Players.LocalPlayer.Character.Humanoid:TakeDamage(1000)
        whisper:Stop()
game:GetService("ReplicatedStorage").GameStats["Player_".. game.Players.LocalPlayer.Name].Total.DeathCause.Value = "Whisperer"
	firesignal(game.ReplicatedStorage.EntityInfo.DeathHint.OnClientEvent, {"You Died To A Unknown Entity Called Whisperer..", "His presence is known when you start to hear whispers.", "The Whispers Are Everywhere And you cant escape it.", "You need to stop moving to avoid it", "as soon you hear it stop moving immediately."}, "Blue")
                 -- Instances:

local ScreenGui = Instance.new("ScreenGui")
local Frame = Instance.new("Frame")
local ImageLabel = Instance.new("ImageLabel")

--Properties:

