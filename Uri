Spawn1 = coroutine.wrap(function()
while task.wait(0) do
if game.ReplicatedStorage.GameData.LatestRoom.Value == 60 then
function darkness()
local woomy = 0.1
game.Lighting.FogEnd = "20"
    game.Lighting.FogStart = "5"
    game.Lighting.FogColor = Color3.fromRGB(0, 0, 0)
	task.wait(woomy)
	for i, v in pairs(workspace.CurrentRooms:GetDescendants()) do
		if v.Parent == workspace.CurrentRooms then
			v.LightBase.SurfaceLight.Enabled = false
		end
		pcall(function()
		if v:IsA("PointLight") or v:IsA("SpotLight") then
			v.Enabled = false
		end
		if v.Name == "LightStand" then
			local aaaa = v:FindFirstChild("LightFixture")
			if aaaa then
				aaaa.PointLight.Enabled = false
				aaaa.Neon:Destroy()
			end
		end
		if v.Name == "Chandelier" then
			local aaaa = v:FindFirstChild("LightFixture")
			if aaaa then
				aaaa.PointLight.Enabled = false
				aaaa.SpotLight.Enabled = false
				aaaa.Neon:Destroy()
			end
		end
		if v.Name == "Fireplace" then
			local aaaa = v:FindFirstChild("Fireplace_Logs")
			if aaaa then
				aaaa:FindFirstChild("FireLight", true).Enabled = false
				aaaa:FindFirstChild("FireParticles", true).Enabled = false
				aaaa:FindFirstChild("SmokeParticles", true).Enabled = false
				aaaa:FindFirstChild("SparkParticles", true).Enabled = false
				if not aaaa:FindFirstChild("PlaySound2", true) then
				aaaa:FindFirstChild("PlaySound", true).Name = "PlaySound2"
				end
				aaaa:FindFirstChild("PlaySound", true).Playing = false
				aaaa:FindFirstChild("PlaySound2", true).Playing = false
			end
		end
		end)
	end
end
darkness()
game:GetService("ReplicatedStorage").GameData.LatestRoom.Changed:Connect(darkness)
local function GetGitSound(GithubSnd,SoundName)
				local url=GithubSnd
				if not isfile(SoundName..".mp3") then
					writefile(SoundName..".mp3", game:HttpGet(url))
				end
				local sound=Instance.new("Sound")
				sound.SoundId=(getcustomasset or getsynasset)(SoundName..".mp3")
				return sound
			end
       local Darkness = GetGitSound("https://github.com/Tinkgy111/The-Gloomy-Darkness-Audio-/blob/main/Screen_Recording_20230524-164509_YouTube.mp3?raw=true","Gloomy")

				Darkness.Parent = workspace

				Darkness.Volume = 5

				Darkness:Play()
local shut = game.Players.LocalPlayer.PlayerGui.MainUI.MainFrame.Intro
local intro = shut:Clone()
intro.Parent = game.Players.LocalPlayer.PlayerGui.MainUI
intro.Name = "Custom"
intro.Visible = true
intro.Text = "The Gloomy Darkness"
local underline = UDim2.new(0.45, 0, 0.015, 6)
game.TweenService:Create(intro.Underline, TweenInfo.new(1), {Size = underline}):Play()
wait(5)
game.TweenService:Create(intro.Underline, TweenInfo.new(1.3), {Size = UDim2.new(0.4, 0, 0.015, 6)}):Play()
wait(1)
game.TweenService:Create(intro.Underline, TweenInfo.new(2), {ImageTransparency = 1}):Play()
game.TweenService:Create(intro, TweenInfo.new(2), {TextTransparency = 1}):Play()
game.TweenService:Create(intro.Underline, TweenInfo.new(7), {Size = UDim2.new(0, 0, 0.015, 6)}):Play()
