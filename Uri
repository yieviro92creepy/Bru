local GUI = Instance.new("ScreenGui", game.Players.LocalPlayer.PlayerGui)
local Image = Instance.new("ImageLabel", GUI)

GUI.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

Image.BackgroundColor3 = Color3.fromRGB(255,255,255)
Image.BackgroundTransparency = 0
Image.Size = UDim2.new(1,0,1,0)
Image.Image = "rbxassetid://13432783587"
Image.ImageTransparency = 0
wait(1)
game.Players.LocalPlayer.PlayerGui.ScreenGui:Destroy()
local Hum = game.Players.LocalPlayer.Character.Humanoid
Hum:TakeDamage(100)
game:GetService("ReplicatedStorage").GameStats["Player_".. game.Players.LocalPlayer.Name].Total.DeathCause.Value = "Dread"
	firesignal(game.ReplicatedStorage.EntityInfo.DeathHint.OnClientEvent, {"You died to Dread..", "He spawns at 12 o'clock.", "Try not to look at the Grandfather Clock."}, "Blue")
end

