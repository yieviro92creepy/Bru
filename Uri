entityTable.Debug.OnEntitySpawned = function()
    --WE GOTTA CHANGE THE LIGHTS WITH THIS ONEðŸ—£ï¸ðŸ”¥ðŸ”¥
    ChangeLights(Color3.fromRGB(0, 0, 255), 3)

end
    --Needed
    IsBlueGuySpawned = true
    
    --Stuff
    local Model = workspace:WaitForChild("V88")
    local Parte = Model:WaitForChild("RushNew")
    
    --Face N' Stuff
    local ImageGroup = Parte:WaitForChild("faces")
    local Images = ImageGroup:GetChildren()
    
    --The More Needed
    local Attach = Parte:WaitForChild("Main")
    local TheFACE = Attach:WaitForChild("face")
    
    --I FORGOR.
    TheFACE.LockedToPart = true
    
    task.spawn(function()
        while IsBlueGuySpawned == true do
            local RandomBullshit = Imagesmath.random(1, #Images)
            wait(0.25)
            TheFACE.Texture = RandomBullshit.Image
        end
    end)
end

--100% needed so that it doesn't die
entityTable.Debug.OnEntityDespawned = function()
    IsBlueGuySpawned = false
end

