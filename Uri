local TW = TweenService:Create(game.Lighting.MainColorCorrection, TweenInfo.new(5),{TintColor = Color3.fromRGB(255, 255, 255)})
TW:Play()
wait(3)
       local ready = GetGitSound("https://github.com/Tonkgy836/Download-/blob/main/Screen_Recording_20230514-122323_YouTube.mp3?raw=true","rigger")

